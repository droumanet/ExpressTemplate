{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": [".\\controllers", ".\\models", ".\\routes"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {},
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }